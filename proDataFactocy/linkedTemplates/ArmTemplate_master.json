{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "proDataFactocy"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"SHIR_Pizza_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SHIR_Pizza'"
		},
		"bronzedata_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'bronzedata'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://projectstorge.dfs.core.windows.net/"
		},
		"SHIR_Pizza_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "HANSRAJ"
		},
		"SHIR_Pizza_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "pizzaz"
		},
		"SHIR_Pizza_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"bronzedata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://projectstorge.dfs.core.windows.net/"
		},
		"sharingSHIR_properties_typeProperties_linkedInfo_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/1884707a-007c-42a8-92b0-3b61acdc684f/resourcegroups/newDataFactory/providers/Microsoft.DataFactory/factories/newDataADF2/integrationruntimes/IRSelfhost"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "proDataFactocy_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"bronzedata_accountKey": {
						"value": "[parameters('bronzedata_accountKey')]"
					},
					"SHIR_Pizza_password": {
						"value": "[parameters('SHIR_Pizza_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"bronzedata_properties_typeProperties_url": {
						"value": "[parameters('bronzedata_properties_typeProperties_url')]"
					},
					"sharingSHIR_properties_typeProperties_linkedInfo_resourceId": {
						"value": "[parameters('sharingSHIR_properties_typeProperties_linkedInfo_resourceId')]"
					},
					"SHIR_Pizza_properties_typeProperties_server": {
						"value": "[parameters('SHIR_Pizza_properties_typeProperties_server')]"
					},
					"SHIR_Pizza_properties_typeProperties_database": {
						"value": "[parameters('SHIR_Pizza_properties_typeProperties_database')]"
					},
					"SHIR_Pizza_properties_typeProperties_userName": {
						"value": "[parameters('SHIR_Pizza_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}